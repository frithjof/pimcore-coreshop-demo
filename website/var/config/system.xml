<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
  <general>
    <timezone>Europe/Berlin</timezone>
    <php_cli></php_cli>
    <domain/>
    <redirect_to_maindomain/>
    <language>en</language>
    <validLanguages>en</validLanguages>
    <fallbackLanguages>
      <en/>
    </fallbackLanguages>
    <theme/>
    <contactemail>t</contactemail>
    <loginscreencustomimage/>
    <disableusagestatistics/>
    <debug/>
    <debug_ip/>
    <http_auth>
      <username/>
      <password/>
    </http_auth>
    <custom_php_logfile>1</custom_php_logfile>
    <debugloglevel>debug</debugloglevel>
    <disable_whoops/>
    <debug_admin_translations/>
    <devmode/>
    <logrecipient/>
    <viewSuffix/>
    <instanceIdentifier/>
  </general>
  <database>
    <adapter>Mysqli</adapter>
    <params>
      <host>localhost</host>
      <username>coreshop_demo</username>
      <password>coreshop_demo</password>
      <dbname>coreshopdemo_pimcore</dbname>
      <port>3306</port>
    </params>
  </database>
  <documents>
    <versions>
      <days/>
      <steps>10</steps>
    </versions>
    <default_controller>default</default_controller>
    <default_action>default</default_action>
    <error_pages>
      <default>/</default>
    </error_pages>
    <createredirectwhenmoved/>
    <allowtrailingslash>no</allowtrailingslash>
    <allowcapitals>no</allowcapitals>
    <generatepreview>1</generatepreview>
    <wkhtmltoimage/>
    <wkhtmltopdf/>
  </documents>
  <objects>
    <versions>
      <days/>
      <steps>10</steps>
    </versions>
  </objects>
  <assets>
    <webdav>
      <hostname/>
    </webdav>
    <versions>
      <days/>
      <steps>10</steps>
    </versions>
    <ffmpeg/>
    <ghostscript/>
    <libreoffice/>
    <pngcrush/>
    <imgmin/>
    <jpegoptim/>
    <pdftotext/>
    <icc_rgb_profile/>
    <icc_cmyk_profile/>
    <hide_edit_image/>
  </assets>
  <services>
    <translate>
      <apikey/>
    </translate>
    <google>
      <client_id/>
      <email/>
      <simpleapikey/>
      <browserapikey/>
    </google>
  </services>
  <cache>
    <enabled/>
    <lifetime/>
    <excludePatterns/>
    <excludeCookie/>
  </cache>
  <outputfilters>
    <less/>
    <lesscpath/>
  </outputfilters>
  <webservice>
    <enabled/>
  </webservice>
  <httpclient>
    <adapter>Zend_Http_Client_Adapter_Socket</adapter>
    <proxy_host/>
    <proxy_port/>
    <proxy_user/>
    <proxy_pass/>
  </httpclient>
  <email>
    <sender>
      <name/>
      <email/>
    </sender>
    <return>
      <name/>
      <email/>
    </return>
    <method/>
    <smtp>
      <host/>
      <port/>
      <ssl/>
      <name/>
      <auth>
        <method/>
        <username></username>
        <password></password>
      </auth>
    </smtp>
    <debug>
      <emailaddresses/>
    </debug>
    <bounce>
      <type/>
      <maildir/>
      <mbox/>
      <imap>
        <host/>
        <port/>
        <username/>
        <password/>
        <ssl/>
      </imap>
    </bounce>
  </email>
  <newsletter>
    <sender>
      <name/>
      <email/>
    </sender>
    <return>
      <name/>
      <email/>
    </return>
    <method/>
    <smtp>
      <host/>
      <port/>
      <ssl/>
      <name/>
      <auth>
        <method/>
        <username/>
        <password/>
      </auth>
    </smtp>
    <usespecific/>
